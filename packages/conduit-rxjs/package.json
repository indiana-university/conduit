  "homepage": "https://github.com/indiana-university/conduit#readme",
  "peerDependencies": {
    "rxjs": ">= 6.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3"
  }
}
