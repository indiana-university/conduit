{
  "name": "conduit-rxjs",
  "version": "0.4.0",
  "description": "RxJS utilities to support UI architectures.",
  "license": "BSD-3-Clause",
  "main": "bundles/conduit-rxjs.umd.min.js",
  "files": [
    "bundles"
  ],
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --mode=development --module-bind js=babel-loader",
